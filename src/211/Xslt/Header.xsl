<?xml version="1.0"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">	<xsl:output method="html"/>	<xsl:template match="/">		<div id="StoreHeader">			<div id="StoreHeaderLogo">				<a href="/">					<img border="0">						<xsl:attribute name="src">							<xsl:value-of select="ExpressLane/header/logo"/>						</xsl:attribute>					</img>				</a>			</div>			<div id="StoreHeaderMiddle">				<div id="StoreInfoDiv" class="clearfix">					<xsl:variable name="StoreID">						<xsl:value-of select="//GenericConsumer/PrimaryStoreID"/>					</xsl:variable>					<span>Shop From Home</span>					<xsl:if test="$StoreID &gt; 0">						<br/>						<xsl:value-of select="//GenericStore[StoreID = $StoreID]/PrimaryAddress"/>&#160;						<xsl:value-of select="//GenericStore[StoreID = $StoreID]/SecondaryAddress"/>&#160;<br/>						<xsl:value-of select="//GenericStore[StoreID = $StoreID]/City"/>, 						<xsl:value-of select="//GenericStore[StoreID = $StoreID]//State"/>&#160;						<xsl:value-of select="//GenericStore[StoreID = $StoreID]/PostalCode"/>&#160;						<xsl:value-of select="//GenericStore[StoreID = $StoreID]/Phone"/>&#160;					</xsl:if>				</div>			</div>			<div id="StoreHeaderRight">				<a class="lightwindow" title="Help Center" params="lightwindow_width=850,lightwindow_close=true" href="https://www.giantfoodstores.com/shareddev/sharedcontent/Ourstores/SFH/FAQs.cfm">					<img src="/Assets/123/images/help_center.gif" border="0" width="131" height="27" alt="Click for help"/>				</a>			</div>			<xsl:comment><![CDATA[[if lt IE 7]>			<script id="dropdowns" type="text/javascript" language="javascript">				sfHover = function() {					var sfEls = document.getElementById("nav").getElementsByTagName("LI");					for (var i=0; i<sfEls.length; i++) {						sfEls[i].onmouseover=function() {							this.className+=" sfhover";						}						sfEls[i].onmouseout=function() {							this.className=this.className.replace(new RegExp(" sfhover\\b"), "");						}					}				}				if (window.attachEvent) window.attachEvent("onload", sfHover);		</script>						<![endif]]]></xsl:comment>			<div id="StoreMenu" class="clearfix">				<ul>							<xsl:for-each select="ExpressLane/header/TopLevelNavigation/MenuItem">								<li>									<a class="nav-link">										<xsl:attribute name="href">											<xsl:value-of select="Link"/>										</xsl:attribute>										<xsl:if test="Target">											<xsl:attribute name="target">												<xsl:value-of select="Target"/>											</xsl:attribute>										</xsl:if>										<xsl:value-of select="Title"/>									</a>								</li>								<li class="navPipe"></li>							</xsl:for-each>				</ul>				<div id="LoginBox">					<p>						<xsl:choose>							<xsl:when test="//GenericConsumer/IsLoggedIn = 'true'">													Welcome |								<a id="A2" runat="server" href="/Profile/SignUp.aspx"> Edit my profile </a>|								<a id="A3" runat="server" href="/Profile/SignOut.aspx"> Log Out </a>							</xsl:when>							<xsl:otherwise>								Not logged in |								<a id="A4" href="/Profile/SignUp.aspx" runat="server"> Register </a>or								<a id="A4" href="/Profile/SignIn.aspx" runat="server"> Sign-In </a>							</xsl:otherwise>						</xsl:choose>					</p>				</div>			</div>				</div>	</xsl:template></xsl:stylesheet><!-- Stylesheet edited using Stylus Studio - (c) 2004-2009. Progress Software Corporation. All rights reserved. -->