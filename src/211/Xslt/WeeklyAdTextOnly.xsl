<?xml version='1.0'?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">	<xsl:output method="html" />	<xsl:template match="/">        <script src="/Assets/Common/js/Circulars/Circulars.js" type="text/javascript"/>		<h2>			Weekly Circular Sale Items		</h2>		<p>			<a id="LinkButtonBackToWeeklyAd" href="WeeklyAd.aspx">BACK TO WEEKLY AD</a>		</p>		<div id="CouponList">			<xsl:for-each select="ExpressLane/StoreCircular/Categories/Category">				<xsl:call-template name="CategoryList" />			</xsl:for-each>		</div>	</xsl:template>	<xsl:template name="CategoryList" match="Category">		<xsl:param name="CategoryID" select="CategoryID" />		<h3>			<xsl:value-of select="CategoryName" />		</h3>		<div>			<table class="GridItem">				<xsl:if test="position()=1"><xsl:attribute name="style">background:none; margin-top:-29px;</xsl:attribute></xsl:if>				<tbody>					<xsl:if test="position()=1">						<tr>							<th scope="col" style="background:none;"> </th>							<th scope="col" style="background:none;"> </th>							<th align="center" scope="col" style="background:none;"> </th>							<th scope="col" style="background:none;"><img src="/Assets/123/Images/BonusCardSmallIcon.gif" border="0" style="float:left;margin-right:4px;" /></th>						</tr>					</xsl:if>					<tr>						<th scope="col"> </th>						<th scope="col">Item</th>						<th align="center" scope="col">Price</th>						<th scope="col">Savings</th>					</tr>					<xsl:for-each select="//ExpressLane/StoreCircular/Pages/StoreCircularPage/Items/StoreCircularItem[CategoryID=$CategoryID]">					<xsl:sort select="Name" data-type="text" order="ascending" />						<tr>							<xsl:attribute name="class">								<xsl:choose>									<xsl:when test="position() mod 2 = 0">GridAlternatingItem</xsl:when>									<xsl:otherwise>GridItem</xsl:otherwise>								</xsl:choose>							</xsl:attribute>							<td class="AddButton">								<a class="button">									<xsl:attribute name="href">javascript:AddToShoppingList(<xsl:value-of select="CircularItemID" />, 8);</xsl:attribute>									<span>Buy</span>								</a>							</td>							<td class="ItemName">								<xsl:value-of select="Name" />							</td>							<td class="ItemPrice">								<xsl:value-of select="PriceString" />							</td>							<td class="ItemSavings">								<xsl:value-of select="AvailableVarieties/GenericProduct/Savings" />							</td>						</tr>					</xsl:for-each>				</tbody>			</table>		</div>	</xsl:template></xsl:stylesheet>