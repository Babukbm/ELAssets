<?xml version='1.0'?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">	<xsl:output method="html" />	<xsl:template match="/" name="SpecialsPageProducts">		<xsl:if test="ExpressLane/Specials/ArrayOfGenericProduct/GenericProduct">			<div class="CategoryHeader">				<h3>					<xsl:choose>						<xsl:when test="ExpressLane/Specials/Category">							<xsl:value-of select="ExpressLane/Specials/Category/CategoryName" />						</xsl:when>						<xsl:otherwise>							<xsl:value-of select="ExpressLane/Specials/Settings/TopNText" />						</xsl:otherwise>					</xsl:choose>				</h3>			</div>			<div>				<xsl:if test="ExpressLane/Specials/Category/CategoryID">					<xsl:if test="ExpressLane/Specials/Settings/DisplayMostPopular = 'True'">						<a>							<xsl:attribute name="href">javascript:DisplayMostPopular();</xsl:attribute>							Display<xsl:value-of select="concat(' ', ExpressLane/Specials/Settings/TopNText)" />						</a>					</xsl:if>				</xsl:if>				<table class="GridItem">					<xsl:if test="position()=1"><xsl:attribute name="style">background:none; margin-top:-29px;</xsl:attribute></xsl:if>					<tbody>						<xsl:if test="position()=1">							<tr>								<th scope="col" style="background:none;"> </th>								<th scope="col" style="background:none;"> </th>								<th scope="col" style="background:none;"> </th>								<th align="center" scope="col" style="background:none;"> </th>								<th scope="col" style="background:none;"><img src="/Assets/123/Images/BonusCardSmallIcon.gif" border="0" style="float:left;margin-right:4px;" /></th>								<th scope="col" style="background:none;"> </th>							</tr>						</xsl:if>						<tr>							<th scope="col"> </th>							<th scope="col"> </th>							<th scope="col">Item</th>							<th scope="col">Price</th>							<th scope="col">Savings</th>							<th scope="col"> </th>						</tr>						<xsl:for-each select="ExpressLane/Specials/ArrayOfGenericProduct/GenericProduct">							<tr>								<xsl:attribute name="class">									<xsl:choose>										<xsl:when test="position() mod 2 = 1">GridAlternatingItem</xsl:when>										<xsl:otherwise>GridItem</xsl:otherwise>									</xsl:choose>								</xsl:attribute>								<td class="AddButton">									<a class="button">										<xsl:attribute name="href">javascript:AddToShoppingList(<xsl:value-of select="ProductID" />, 3);</xsl:attribute>										<span>Buy</span>									</a>								</td>								<td class="ProductPageImage">									<img alt="">										<xsl:attribute name="src">										<xsl:choose>											<xsl:when test="SmallImageURL != ''"><xsl:value-of select="SmallImageURL" /></xsl:when>											<xsl:otherwise>##root##/Assets/Common/Images/no_image.gif</xsl:otherwise>										</xsl:choose>										</xsl:attribute>									</img>																</td>								<td class="ItemName">									<xsl:if test="BrandName">										<b><xsl:value-of select="concat(BrandName, ' ')" /></b>									</xsl:if>									<xsl:value-of select="Description" />								</td>								<td class="ItemPrice">									<xsl:value-of select="SalePriceString" />								</td>								<td class="ItemSavings">									<xsl:choose>										<xsl:when test="ProductImageURL">											<img alt="">												<xsl:attribute name="src">													<xsl:value-of select="ProductImageURL" />												</xsl:attribute>											</img>										</xsl:when>										<xsl:otherwise>											<xsl:value-of select="Savings" />										</xsl:otherwise>									</xsl:choose>								</td>								<td class="ItemSavings">									<xsl:value-of select="IconsHTML" disable-output-escaping="yes" />								</td>							</tr>						</xsl:for-each>					</tbody>				</table>			</div>		</xsl:if>	</xsl:template></xsl:stylesheet>