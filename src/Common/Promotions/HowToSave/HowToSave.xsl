<?xml version="1.0" encoding="utf-8"?><xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">	<xsl:output method="html"/>	<xsl:template match="/">		<script type="text/javascript" src="http://villagemarket.beta.groceryshopping.net/Assets/Common/Js/swfobject2.js"/>		<script type="text/javascript" src="http://villagemarket.beta.groceryshopping.net/Assets/Common/Js/prototype.js"/>		<script type="text/javascript"><![CDATA[		var activeBut = 'but1';		var bodyTextArray = [];			]]><xsl:for-each select="//Section"><![CDATA[		bodyTextArray[]]><xsl:value-of select="position()-1"/><![CDATA[] = "<p>]]><xsl:value-of select="BodyText"/><![CDATA[</p>";		]]></xsl:for-each><![CDATA[				function init() { 			$('but1').addClassName('linkActive'); 			$("bodytext").innerHTML=(bodyTextArray[0]);			}		window.onload = init;									function clickColor(theSpan,index){			if(activeBut != theSpan){				$(theSpan).addClassName('linkActive');				$(activeBut).removeClassName('linkActive');				$("bodytext").innerHTML=(bodyTextArray[(index) - 1]);				activeBut = theSpan;				}			}	]]></script>		<style>	@import url(<xsl:value-of select="//LandingPage/Stylesheet"/>);</style>			<div class="promotionHeader">				<h1>When the going gets tough,<br/>    the tough <span>shop smarter.</span></h1>				<p>How to use your grocery website to stretch <br/>your grocery budget <span>10, 20, 30%</span> or more!</p>			</div>			<div id="failover">				<div class="linkColumn">					<xsl:for-each select="//Section">						<xsl:if test="position() &lt; 4">							<li class="linkActive">								<xsl:attribute name="onclick">clickColor('<xsl:value-of select="concat('but',position())"/>','<xsl:value-of select="position()"/>')</xsl:attribute>								<a href="#">									<span>										<xsl:attribute name="id">but<xsl:value-of select="position()"/></xsl:attribute>										<xsl:value-of select="concat(position(),'. ',Title)"/>									</span>								</a>							</li>						</xsl:if>					</xsl:for-each>				</div>				<div class="linkColumn">					<xsl:for-each select="//Section">						<xsl:if test="(position() &gt; 3) and (position() &lt; 7)">							<li class="linkActive">								<xsl:attribute name="onclick">clickColor('<xsl:value-of select="concat('but',position())"/>','<xsl:value-of select="position()"/>')</xsl:attribute>								<a href="#">									<span>										<xsl:attribute name="id">but<xsl:value-of select="position()"/></xsl:attribute>										<xsl:value-of select="concat(position(),'. ',Title)"/>									</span>								</a>							</li>						</xsl:if>					</xsl:for-each>				</div>				<div id="bodytext">				</div>			</div>	</xsl:template></xsl:stylesheet>